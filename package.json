{
  "name": "@idevelopthings/reflect-extensions",
  "version": "0.0.3",
  "type": "module",
  "description": "Reflect utils/extensions",
  "keywords": [
    "Reflect",
    "Reflection",
    "javascript",
    "typescript"
  ],
  "author": {
    "name": "iDevelopThings",
    "url": "https://github.com/idevelopthings"
  },
  "readme": "readme.md",
  "repository": {
    "type": "git",
